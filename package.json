{
  "name": "textmate-grammar-uri",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.15.6",
  "description": "TextMate grammar for URI",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/textmate-grammar-uri#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/textmate-grammar-uri.git"
  },
  "bugs": "https://github.com/antfu/textmate-grammar-uri/issues",
  "keywords": [
    "textmate-grammar"
  ],
  "sideEffects": false,
  "exports": {
    "./grammars/*": "./grammars/*"
  },
  "files": [
    "grammars"
  ],
  "scripts": {
    "build": "esno src/index.ts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "play": "nr -C playground dev",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.12.6",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.6",
    "rimraf": "^5.0.5",
    "shiki": "^1.2.4",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.17.0",
    "typescript": "^5.4.4",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue": "^3.4.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
